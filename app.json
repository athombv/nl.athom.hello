{
    "id": "nl.athom.hello",
    "version": "1.0.0",
    "compatibility": "0.x || 1.x",
    "category": "internet",
    "name": {
        "en": "Hello World!",
        "nl": "Hallo wereld!"
    },
    "description": {
        "en": "Example app for Homey",
        "nl": "Dit is een voorbeeld :)",
        "fr": "baguette?"
    },
    "author": {
        "name": "Emile Nijssen",
        "email": "emile@athom.nl"
    },
    "permissions": [
        "homey:manager:speech-input",
        "homey:manager:speech-output"
    ],
    "speech": [
        {
            "id": "hello",
            "importance": 0.6,
            "triggers": {
                "en": [
                    "hello",
                    "hi",
                    "hey"
                ]
            }
        }
    ]
}